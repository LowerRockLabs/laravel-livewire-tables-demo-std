{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "guzzlehttp/guzzle": "^6.0|^7.0.1",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "laravel/pint": "^1.6",
        "livewire/livewire": "^2.6",
        "maatwebsite/excel": "^3.1",
        "rappasoft/laravel-livewire-tables": "dev-dusk-tests"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "barryvdh/laravel-debugbar": "^2.0|^3.6",
        "spatie/laravel-ignition": "^1.0|^2.0",
        "fakerphp/faker": "^1.7^|^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.0|^7.0",
        "orchestra/testbench": "^6.13|^7.0|^8.0",
        "brianium/paratest": "^4.0|^5.0|^6.1|^7.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rappasoft\\LaravelLivewireTables\\Tests\\Standard\\": "./tests/Standard",
            "Rappasoft\\LaravelLivewireTables\\Tests\\Dusk\\": "./tests/Dusk"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clear-all": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "composer dumpautoload -o"
        ],
        "cache-all": [
            "@php artisan config:cache",
            "@php artisan route:cache"
        ],
        "reset": [
            "composer clear-all",
            "composer cache-all"
        ],
        "pint": "./vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/LowerRockLabs/laravel-livewire-tables.git"
        }
    ]
}
